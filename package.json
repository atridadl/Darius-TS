{
  "name": "darius",
  "version": "1.0.2",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently \"bun run --hot --inspect src/main.ts\" \"bun run tw:dev\"",
    "start": "bun run src/main.ts",
    "tw:ci": "bun tailwindcss -i ./src/base.css -o ./public/css/styles.css",
    "tw:dev": "bun tailwindcss -i ./src/base.css -o ./public/css/styles.css --watch"
  },
  "dependencies": {
    "@elysiajs/html": "^0.7.3",
    "@elysiajs/static": "^0.7.1",
    "@elysiajs/swagger": "^0.7.4",
    "elysia": "latest",
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "latest",
    "bun-types": "latest",
    "daisyui": "^3.9.4",
    "tailwindcss": "^3.3.5",
    "concurrently": "^8.2.2"
  },
  "module": "src/main.ts"
}
